<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
	<add key="tdevere_nuget" value="https://pkgs.dev.azure.com/tdevere/_packaging/tdevere/nuget/v3/index.json" />
  </packageSources>
      <packageSourceCredentials>
        <tdevere_nuget>
            <add key="Username" value="tdevere"/>
            <add key="ClearTextPassword" value="%APPCENTER_SOURCE_DIRECTORY%"/>
        </tdevere_nuget>
    </packageSourceCredentials>
  <packageRestore>
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>
  <bindingRedirects>
    <add key="skip" value="False" />
  </bindingRedirects>
  <packageManagement>
    <add key="format" value="1" />
    <add key="disabled" value="False" />
  </packageManagement>
</configuration>